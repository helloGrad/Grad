<% if(pageVo.page<5) { %>
				<a  class="btn btn-default disabled"
								href="#" onclick="pageList('<%=type%>','<%=order%>',<%=pageVo.startPage-5%>,'','<%=ttype%>');">&lt;&lt;</a>
								
<% } else { %>
		<a class="btn btn-default"
								href="#" onclick="pageList('<%=type%>','<%=order%>',<%=pageVo.startPage-5%>,'','<%=ttype%>');">&lt;&lt;</a>
<% } 
		if(pageVo.page == 1) { %>
		
		<a  class="btn btn-default disabled"
								href="#" onclick="pageList('<%=type%>','<%=order%>',<%=pageVo.page-1%>,'','<%=ttype%>');">&lt;</a>
<% }
		else { %>
						<a  class="btn btn-default"
								href="#" onclick="pageList('<%=type%>','<%=order%>',<%=pageVo.page-1%>,'','<%=ttype%>');">&lt;</a>
								
<%  }  for(i=pageVo.startPage; i<=pageVo.endPage ; i++) { 
					if(pageVo.page==i) {%>
						<a  class="btn btn-default disabled numbtn" href="#" 
								onclick="pageList('<%=type%>','<%=order%>',<%=i%>,'','<%=ttype%>');"><%=i%></a>
					<% } else { %>
					
						<a  class="btn btn-default" href="#" 
								onclick="pageList('<%=type%>','<%=order%>',<%=i%>,'','<%=ttype%>');" ><%=i%></a>
								
<% 						}
		} 
		
		if(pageVo.page==pageVo.totalPage) {%>
			<a  class="btn btn-default disabled"
								href="#" 
								onclick="pageList('<%=type%>','<%=order%>',<%=pageVo.page+1%>,'','<%=ttype%>');" >&gt;</a>
								
<%     }    else {  %>
			<a  class="btn btn-default"
							href="#" 
								onclick="pageList('<%=type%>','<%=order%>',<%=pageVo.page+1%>,'','<%=ttype%>');">&gt;</a>
								
								
<%  }   if(pageVo.endPage >= pageVo.totalPage) { %>
				<a class="btn btn-default disabled"
								href="#" 
								onclick="pageList('<%=type%>','<%=order%>',<%=pageVo.startPage+5%>,'','<%=ttype%>');">&gt;&gt;</a>
								
<%  }  else {  %>
			<a  class="btn btn-default"
						href="#" 
					onclick="pageList('<%=type%>','<%=order%>',<%=pageVo.startPage+5%>,'','<%=ttype%>');">&gt;&gt;</a>
					
					
<% } %>
		
